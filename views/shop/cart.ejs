<%- include('../includes/head.ejs') %>
  </head>

  <body>
    <%- include('../includes/navigation.ejs') %>
    <main>
      <% if (products.length > 0) { %>
        <ul>
          <% products.forEach(p => { %>
            <li><a href="/products/<%= p.productData.id%>"><%= p.productData.title %></a>(<%= p.qty %>)</li>
          <% }) %>
        </ul>
      <% } else { %>
        <h1>
          No products in the cart !
        </h1>
      <% } %>
    </main>
    <%- include('../includes/end.ejs') %>